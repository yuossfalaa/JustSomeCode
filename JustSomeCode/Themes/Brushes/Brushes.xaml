<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

                    <!--Primary Brushs-->
                    <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#F8550E"/>
                    <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="White"/>

                    <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="#E30057"/>
                    <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="White"/>

                    <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="#9F09Ac"/>
                    <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="White"/>
                
                   <!--Secondary Brushs-->
                   <SolidColorBrush x:Key="SecondaryHueLightBrush" Color="#0f0f10" />
                   <SolidColorBrush x:Key="SecondaryHueLightForegroundBrush" Color="White"/>

                   <SolidColorBrush x:Key="SecondaryHueMidBrush" Color="#232427"/>
                   <SolidColorBrush x:Key="SecondaryHueMidForegroundBrush" Color="White"/>

                   <SolidColorBrush x:Key="SecondaryHueDarkBrush" Color="#1b1c20"/>
                   <SolidColorBrush x:Key="SecondaryHueDarkForegroundBrush" Color="White"/>

    <SolidColorBrush x:Key="BorderBruch" Color="White" Opacity="0.90"/>
    <SolidColorBrush x:Key="BorderBruchBlack" Color="Black" Opacity="0.2"/>

</ResourceDictionary>
